<node>
  <interface name="com.endlessm.HackableApp">
    <method name="PulseFlipToHackButton">
      <arg type="b" direction="in" name="enable"/>
    </method>
    <!-- AppId: The well-known-name of the application. -->
    <property name="AppId" type="s" access="read"/>
    <!-- State: The current state of the application. 0: APP, 1: TOOLBOX.-->
    <property name="State" type="u" access="read"/>
    <!-- ToolboxVisible: Whether the toolbox of the app is visible or not.-->
    <property name="ToolboxVisible" type="b" access="readwrite"/>
  </interface>
</node>
